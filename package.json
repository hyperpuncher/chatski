{
	"name": "chatski",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "bun -b vite dev --host",
		"build": "bun -b vite build",
		"preview": "bun -b vite preview --host",
		"start": "bun -b ./build",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check",
		"check:watch": "svelte-kit sync && svelte-check --watch",
		"format": "bun run format:oxfmt ; bun run format:prettier",
		"format:oxfmt": "oxfmt",
		"format:prettier": "prettier --log-level error --write '**/*.svelte'",
		"lint": "oxlint",
		"lint:fix": "oxlint --fix",
		"clean": "rm -rf node_modules/ .svelte-kit/ build/ bun.lock"
	},
	"dependencies": {
		"@ai-sdk/mcp": "^1.0.21",
		"@ai-sdk/svelte": "^4.0.97",
		"@fontsource-variable/cascadia-code": "^5.2.2",
		"@fontsource-variable/inter": "^5.2.8",
		"@openrouter/ai-sdk-provider": "2.2.3",
		"ai": "^6.0.97",
		"better-auth": "1.5.0-beta.16",
		"mode-watcher": "^1.1.0",
		"runed": "^0.37.1",
		"svelte-streamdown": "^3.0.1",
		"unstorage": "^1.17.4",
		"uuidv7": "^1.1.0",
		"valibot": "^1.2.0"
	},
	"devDependencies": {
		"@ai-sdk/devtools": "^0.0.15",
		"@internationalized/date": "^3.11.0",
		"@lucide/svelte": "^0.575.0",
		"@sveltejs/adapter-node": "^5.5.3",
		"@sveltejs/kit": "^2.53.0",
		"@sveltejs/vite-plugin-svelte": "7.0.0-next.0",
		"@tailwindcss/vite": "^4.2.0",
		"@types/bun": "^1.3.9",
		"@types/node": "^25.3.0",
		"@typescript/native-preview": "^7.0.0-dev.20260221.1",
		"@vite-pwa/sveltekit": "^1.1.0",
		"bits-ui": "^2.16.2",
		"clsx": "^2.1.1",
		"eruda": "^3.4.3",
		"oxfmt": "^0.34.0",
		"oxlint": "^1.49.0",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.5.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"rollup-plugin-visualizer": "^7.0.0",
		"svelte": "5.49.1",
		"svelte-check": "^4.4.3",
		"svelte-render-scan": "^1.1.0",
		"svelte-sonner": "^1.0.7",
		"tailwind-merge": "^3.5.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.2.0",
		"tw-animate-css": "^1.4.0",
		"vite": "^7.2.6",
		"vite-plugin-devtools-json": "^1.0.0"
	},
	"overrides": {
		"vite": "^8.0.0-beta.15"
	}
}
