{
	"name": "chatski",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "bun -b vite dev --host",
		"build": "bun -b vite build",
		"preview": "bun -b vite preview --host",
		"start": "bun -b ./build",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check",
		"check:watch": "svelte-kit sync && svelte-check --watch",
		"format": "prettier --write . ; biome check --linter-enabled=false --write . ; rustywind --write .",
		"lint": "oxlint",
		"lint:fix": "oxlint --fix",
		"clean": "rm -rf node_modules/ .svelte-kit/ build/ bun.lock"
	},
	"devDependencies": {
		"@ai-sdk/devtools": "^0.0.14",
		"@internationalized/date": "^3.11.0",
		"@lucide/svelte": "^0.564.0",
